FILTERS( COUNTRY_INDICATOR Varchar 'WarrantyDataDetailReport20070622.COUNTRY_INDICATOR', GL_ACCOUNT Varchar 
'WarrantyDataDetailReport20070622.GL_ACCOUNT' )

WITH (prmBegin_Trx_Date Timestamp, prmEnd_Trx_Date Timestamp) 

SELECT WarrantyDataDetailReport20070622.EXPENSE_AMOUNT AS EXPENSE_AMOUNT, 
0  AS EXPENSE_AMOUNT_DEC, 
WarrantyDataDetailReport20070622.CLAIM_NUMBER AS CLAIM_NUMBER, WarrantyDataDetailReport20070622.STEP_NUMBER AS 
STEP_NUMBER, WarrantyDataDetailReport20070622.BUSINESS_UNIT AS BUSINESS_UNIT, 
WarrantyDataDetailReport20070622.RESERVE_GROUP AS RESERVE_GROUP, WarrantyDataDetailReport20070622.CLAIM_TYPE AS 
CLAIM_TYPE, WarrantyDataDetailReport20070622.MATERIAL_LABOR AS MATERIAL_LABOR, 
WarrantyDataDetailReport20070622.GL_ACCOUNT AS GL_ACCOUNT, WarrantyDataDetailReport20070622.EXPENSE_TYPE_DESCR AS 
EXPENSE_TYPE_DESCR, WarrantyDataDetailReport20070622.OFFICE_NAME AS OFFICE_NAME, 
WarrantyDataDetailReport20070622.GL_PROD_CODE AS GL_PROD_CODE, WarrantyDataDetailReport20070622.MANF_PROD_CODE AS 
MANF_PROD_CODE, WarrantyDataDetailReport20070622.COMPANY_OWNED AS COMPANY_OWNED, 
WarrantyDataDetailReport20070622.CUSTOMER_NUMBER AS CUSTOMER_NUMBER, WarrantyDataDetailReport20070622.CUSTOMER_NAME 
AS CUSTOMER_NAME, WarrantyDataDetailReport20070622.INTERNAL_EXTERNAL AS INTERNAL_EXTERNAL, 
WarrantyDataDetailReport20070622.TRX_DATE AS TRX_DATE, WarrantyDataDetailReport20070622.TRX_YEAR AS TRX_YEAR, 
WarrantyDataDetailReport20070622.TRX_MONTH AS TRX_MONTH, WarrantyDataDetailReport20070622.INTMONTHS_TRX_TO_BASE AS 
INTMONTHS_TRX_TO_BASE, WarrantyDataDetailReport20070622.INTMONTHS_SHIP_TO_BASE AS INTMONTHS_SHIP_TO_BASE, 
WarrantyDataDetailReport20070622.SHIP_DATE AS SHIP_DATE, WarrantyDataDetailReport20070622.SHIP_YEAR_MONTH AS 
SHIP_YEAR_MONTH, WarrantyDataDetailReport20070622.INTMONTHS_SHIP_TO_TRX AS INTMONTHS_SHIP_TO_TRX, 
WarrantyDataDetailReport20070622.START_DATE AS START_DATE, WarrantyDataDetailReport20070622.INTMONTHS_START_TO_TRX 
AS INTMONTHS_START_TO_TRX, WarrantyDataDetailReport20070622.FAIL_DATE AS FAIL_DATE, 
WarrantyDataDetailReport20070622.INTMONTHS_FAIL_TO_TRX AS INTMONTHS_FAIL_TO_TRX, 
WarrantyDataDetailReport20070622.WARRANTY_TYPE AS WARRANTY_TYPE, WarrantyDataDetailReport20070622.WARRANTY_DURATION 
AS WARRANTY_DURATION, WarrantyDataDetailReport20070622.CURRENCY AS CURRENCY, 
WarrantyDataDetailReport20070622.COUNTRY_INDICATOR AS COUNTRY_INDICATOR, 
WarrantyDataDetailReport20070622.RETROFIT_ID AS RETROFIT_ID, WarrantyDataDetailReport20070622.GL_DEPT AS GL_DEPT, 
WarrantyDataDetailReport20070622.IN_RESERVE_PERCENT AS IN_RESERVE_PERCENT, WarrantyDataDetailReport20070622.TRX_LAG 
AS TRX_LAG, WarrantyDataDetailReport20070622.TRXYEARMONTH AS TRXYEARMONTH

FROM "InformationObjects/WarrantyConcessionIO/Information Objects/WarrantyDataDetailReport.iob" AS 
WarrantyDataDetailReport20070622

WHERE :?COUNTRY_INDICATOR
AND :?GL_ACCOUNT
AND WarrantyDataDetailReport20070622.TRX_DATE  >=:prmBegin_Trx_Date
AND WarrantyDataDetailReport20070622.TRX_DATE  <=:prmEnd_Trx_Date


ORDER BY COUNTRY_INDICATOR ASC ,WARRANTY_DURATION ASC,GL_ACCOUNT Asc ,TRX_YEAR ASC,TRX_MONTH ASC, RESERVE_GROUP ASC,CLAIM_TYPE ASC